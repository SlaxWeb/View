{
    "name": "slaxweb/view",
    "description": "View library for SlaxWeb Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
        "php":  ">=7.0.0",
        "symfony/http-foundation": "~3.0",
        "slaxweb/logger": "dev-develop",
        "slaxweb/config": "dev-develop"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "codeception/specify": "*",
        "behat/gherkin": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "SlaxWeb\\View\\": "src/",
            "SlaxWeb\\View\\Test\\Unit\\": "tests/unit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "minimum-stability": "dev"
}
